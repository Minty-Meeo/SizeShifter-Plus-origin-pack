{
    "type": "origins:multiple",
    "name": "Resize Other",
    "description": "You can also resize any living entity at the cost of exhausting your hunger bar.  This power has no effect on ender dragons, withers, or wardens.",
    "badges": [
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/active.png",
            "text": [
                "Use [",
                {
                    "keybind": "key.origins.ternary_active"
                },
                "] to grow the target in your crosshair (up to 8x scale)."
            ]
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/active.png",
            "text": [
                "Use [",
                {
                    "keybind": "key.origins.quaternary_active"
                },
                "] to shrink the target in your crosshair (down to 1/256x scale)."
            ]
        }
    ],
